{
  "license": "MIT",
  "name": "jsapp-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "build": "webpack --progress --config webpack.config.js",
    "build:prod": "webpack --progress --config webpack.config.js --mode production",
    "lint": "yarn tslint && yarn eslint && yarn stylelint",
    "prettier": "echo 'ðŸ’ˆ Listing files to format. Run yarn prettier:write to write.\n' && prettier --list-different src/**/*.js src/**/*.jsx src/**/*.ts src/**/*.tsx  test/**/*.js test/**/*.jsx *.config.js",
    "prettier:write": "prettier --write src/**/*.js src/**/*.jsx src/**/*.ts src/**/*.tsx test/**/*.js test/**/*.jsx *.config.js",
    "test": "karma start --single-run",
    "test:full": "yarn test && yarn test:functional",
    "test:functional": "mocha test/functional/*.functional.jsx",
    "test:watch": "karma start",
    "flow": "flow check",
    "eslint": "eslint . --ext .js --ext .jsx",
    "eslint:fix": "eslint . --ext .js --ext .jsx --fix",
    "tslint": "tslint -c tslint.json src/**/*.ts src/**/*.tsx",
    "tslint:fix": "tslint -c tslint.json src/**/*.ts src/**/*.tsx --fix",
    "stylelint": "stylelint --config .stylelintrc-noprocessor '**/*.css' && stylelint '**/*.tsx' '**/*.jsx' '**/*theme.ts'",
    "d": "yarn dev",
    "dev": "webpack-dev-server --progress --colors"
  },
  "dependencies": {
    "emotion": "^9.0.2",
    "emotion-theming": "^9.0.0",
    "history": "^4.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-emotion": "^9.0.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/preset-stage-3": "^7.0.0-beta.40",
    "@babel/preset-typescript": "^7.0.0-beta.40",
    "@types/react": "^16.0.14",
    "@types/react-dom": "^16.0.1",
    "@types/react-redux": "^5.0.12",
    "@types/react-router-dom": "^4.2.0",
    "@types/react-router-redux": "^5.0.8",
    "autoprefixer": "^8.1.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^8.0.0-beta.2",
    "babel-preset-airbnb": "^2.4.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.3",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^3.0.6",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-nightmare": "^0.4.13",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.5",
    "mocha": "^5.0.4",
    "nightmare": "^3.0.0",
    "prettier": "^1.7.4",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "sinon": "^4.4.4",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-eslint-rules": "^5.1.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1"
  }
}
