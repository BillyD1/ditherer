{
  "license": "MIT",
  "name": "jsapp-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "build:prod": "NODE_ENV=production webpack --progress --config webpack.config.js --mode production",
    "build": "webpack --progress --config webpack.config.js",
    "clean": "rm -rf dist",
    "d:hot": "yarn dev:hot",
    "d": "yarn dev",
    "deploy:github:prebuilt": "gh-pages --dist dist",
    "deploy:github": "yarn clean && yarn build:prod && yarn deploy:github:prebuilt",
    "dev:hot": "yarn dev --hot --inline",
    "dev": "webpack-dev-server --progress --colors",
    "eslint:fix": "eslint . --ext .js --ext .jsx --fix",
    "eslint": "eslint . --ext .js --ext .jsx",
    "lint": "yarn tslint && yarn eslint && yarn stylelint && yarn tsc:check",
    "prettier:fix": "prettier --write src/**/*.js src/**/*.jsx src/**/*.ts src/**/*.tsx test/**/*.js test/**/*.jsx *.config.js",
    "prettier": "echo 'ðŸ’ˆ Listing files to format. Run yarn prettier:fix to write.\n' && prettier --list-different src/**/*.js src/**/*.jsx src/**/*.ts src/**/*.tsx  test/**/*.js test/**/*.jsx *.config.js",
    "stylelint": "stylelint --config .stylelintrc-noprocessor '**/*.css' && stylelint '**/*.tsx' '**/*.jsx' '**/*theme.ts'",
    "test:full": "yarn test && yarn test:functional",
    "test:functional": "mocha test/functional/*.functional.{js,jsx}",
    "test:watch": "karma start",
    "test:xvfb": "DISPLAY=':99.0' Xvfb :99 -screen 0 1024x768x24 > /dev/null 2>&1 &",
    "test": "karma start --single-run",
    "tsc:check:no-error": "tsc --noEmit --skipLibCheck || true",
    "tsc:check": "tsc --noEmit --skipLibCheck",
    "tslint:fix": "yarn tslint --fix",
    "tslint": "tslint -c tslint.json src/**/*.ts src/**/*.tsx"
  },
  "dependencies": {
    "emotion": "^9.1.2",
    "emotion-theming": "^9.1.2",
    "history": "^4.7.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-emotion": "^9.1.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.46",
    "@babel/preset-stage-3": "^7.0.0-beta.46",
    "@babel/preset-typescript": "^7.0.0-beta.46",
    "@types/chai": "^4.1.3",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/mocha": "^5.2.1",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/react-router-dom": "^4.2.7",
    "@types/react-router-redux": "^5.0.15",
    "@types/redux-mock-store": "^0.0.13",
    "@types/webpack-env": "^1.13.6",
    "autoprefixer": "^8.6.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.2",
    "chai": "^4.1.2",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.11",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "^1.1.5",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-nightmare": "^0.4.15",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.2.0",
    "nightmare": "^3.0.1",
